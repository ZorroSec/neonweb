<div class="container">
  {{#each posts}}
    <div class="card mb-3">
        <div class="card-body">
          <div class="card-title">
            <h6>
              <a href="/post/{{titulo}}">{{titulo}}</a>
            </h6>
          </div>
          <div class="card-subtitle">
            <a href="/{{nome}}">{{nome}}</a>
          </div>
          <div class="card-text">
            {{createdAt}}
            <br>
            {{updatedAt}}
          </div>
        </div>
      </div>
  {{/each}}
</div>